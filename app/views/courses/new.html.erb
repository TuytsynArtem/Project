<h2>добавление курса</h2>\
<%=form_for @courses do |f|  %>
  <div>
    <%=f.label :Регистрация %>
  </div>
  <div >

    <%=f.text_field :name    %>

    <%=f.label :name%>
  </div>
  <div>
    <%=f.text_field :last_name%>

    <%=f.label :last_name%>
  </div>
  <div>
    <%=f.text_field :login   %>
    <%=f.label :login%>
  </div>
  <div>
    <%=f.text_field :password%>

    <%=f.label :password %>
  </div>
  <div>
    <%=f.submit 'добавить курс' %>
  </div>
<% end %>
</div>
